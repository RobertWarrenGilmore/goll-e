//
// node-inputs-outputs-with-style-metadata.gcl
// Sample that demonstrates how to create inputs and outputs with metadata
// and styles.
// 

vertex newYork
{
    output jfk_airport
    {
        attribute "size": "really big"
        class "someStyle"
    }
    
    input port_of_new_york
    {
        attribute "size": "le huge"
        class "coolStyle"
    }
}
