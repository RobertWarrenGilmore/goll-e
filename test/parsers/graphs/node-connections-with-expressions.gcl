//
// node-connections-with-expressions.gcl
// Sample that demonstrates how to create connections between nodes that have
// metadata and style information.
//

vertex california
{
    input portOfLA
    output lax
}

vertex newYork
{
    input jfk
    output portOfNYC
}

edge ports newYork.portOfNyc -> california.portOfLA
{
    class "oceanStyle"
    attribute "water": "yes"
}
